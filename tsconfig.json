{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": false,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "build",
    "incremental": true,
    "resolveJsonModule": true,
    "strict": true
  }
}
